<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dogproject">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:usesCleartextTraffic="true"
        android:allowBackup="true"
        android:icon="@mipmap/dog"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".activitys.LoaderActivity"
            android:label="@string/app_name"
            android:theme="@style/LoaderTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activitys.MainActivity">

        </activity>
        <activity android:name=".activitys.SubBreedsActivity"/>
        <activity android:name=".activitys.ImageActivity"/>
        <activity android:name=".activitys.FavouriteActivity"/>
    </application>

</manifest>